{
  "Server": {
    "Port": 8080
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_URL};Port=5432;Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};SSL Mode=Require;Trust Server Certificate=true;"
  },
  "Frontend": {
    "Url": "${FRONTEND_URL}"
  },
  "GoogleGemini": {
    "ApiKey": "${GEMINI_API_KEY}"
  },
  "Resend": {
    "ApiKey": "${RESEND_API_KEY}"
  }
}
